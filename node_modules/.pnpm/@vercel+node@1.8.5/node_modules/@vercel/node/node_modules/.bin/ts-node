#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/aaron/sandbox/learnNode/starter-files/node_modules/.pnpm/ts-node@8.9.1_typescript@3.9.3/node_modules/ts-node/dist/node_modules:/home/aaron/sandbox/learnNode/starter-files/node_modules/.pnpm/ts-node@8.9.1_typescript@3.9.3/node_modules/ts-node/node_modules:/home/aaron/sandbox/learnNode/starter-files/node_modules/.pnpm/ts-node@8.9.1_typescript@3.9.3/node_modules:/home/aaron/sandbox/learnNode/starter-files/node_modules/.pnpm/node_modules:/home/aaron/sandbox/learnNode/starter-files/node_modules:/home/aaron/sandbox/learnNode/node_modules:/home/aaron/sandbox/node_modules:/home/aaron/node_modules:/home/node_modules:/node_modules:/home/aaron/sandbox/learnNode/starter-files/node_modules/.pnpm/@vercel+node@1.8.5/node_modules/@vercel/node/node_modules:/home/aaron/sandbox/learnNode/starter-files/node_modules/.pnpm/@vercel+node@1.8.5/node_modules/@vercel/node_modules:/home/aaron/sandbox/learnNode/starter-files/node_modules/.pnpm/@vercel+node@1.8.5/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/aaron/sandbox/learnNode/starter-files/node_modules/.pnpm/ts-node@8.9.1_typescript@3.9.3/node_modules/ts-node/dist/node_modules:/home/aaron/sandbox/learnNode/starter-files/node_modules/.pnpm/ts-node@8.9.1_typescript@3.9.3/node_modules/ts-node/node_modules:/home/aaron/sandbox/learnNode/starter-files/node_modules/.pnpm/ts-node@8.9.1_typescript@3.9.3/node_modules:/home/aaron/sandbox/learnNode/starter-files/node_modules/.pnpm/node_modules:/home/aaron/sandbox/learnNode/starter-files/node_modules:/home/aaron/sandbox/learnNode/node_modules:/home/aaron/sandbox/node_modules:/home/aaron/node_modules:/home/node_modules:/node_modules:/home/aaron/sandbox/learnNode/starter-files/node_modules/.pnpm/@vercel+node@1.8.5/node_modules/@vercel/node/node_modules:/home/aaron/sandbox/learnNode/starter-files/node_modules/.pnpm/@vercel+node@1.8.5/node_modules/@vercel/node_modules:/home/aaron/sandbox/learnNode/starter-files/node_modules/.pnpm/@vercel+node@1.8.5/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../ts-node@8.9.1_typescript@3.9.3/node_modules/ts-node/dist/bin.js" "$@"
else
  exec node  "$basedir/../../../../../../ts-node@8.9.1_typescript@3.9.3/node_modules/ts-node/dist/bin.js" "$@"
fi
