#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/aaron/sandbox/learnNode/starter-files/node_modules/.pnpm/he@1.2.0/node_modules/he/bin/node_modules:/home/aaron/sandbox/learnNode/starter-files/node_modules/.pnpm/he@1.2.0/node_modules/he/node_modules:/home/aaron/sandbox/learnNode/starter-files/node_modules/.pnpm/he@1.2.0/node_modules:/home/aaron/sandbox/learnNode/starter-files/node_modules/.pnpm/node_modules:/home/aaron/sandbox/learnNode/starter-files/node_modules:/home/aaron/sandbox/learnNode/node_modules:/home/aaron/sandbox/node_modules:/home/aaron/node_modules:/home/node_modules:/node_modules:/home/aaron/sandbox/learnNode/starter-files/node_modules/.pnpm/html-to-text@8.0.0/node_modules/html-to-text/node_modules:/home/aaron/sandbox/learnNode/starter-files/node_modules/.pnpm/html-to-text@8.0.0/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/aaron/sandbox/learnNode/starter-files/node_modules/.pnpm/he@1.2.0/node_modules/he/bin/node_modules:/home/aaron/sandbox/learnNode/starter-files/node_modules/.pnpm/he@1.2.0/node_modules/he/node_modules:/home/aaron/sandbox/learnNode/starter-files/node_modules/.pnpm/he@1.2.0/node_modules:/home/aaron/sandbox/learnNode/starter-files/node_modules/.pnpm/node_modules:/home/aaron/sandbox/learnNode/starter-files/node_modules:/home/aaron/sandbox/learnNode/node_modules:/home/aaron/sandbox/node_modules:/home/aaron/node_modules:/home/node_modules:/node_modules:/home/aaron/sandbox/learnNode/starter-files/node_modules/.pnpm/html-to-text@8.0.0/node_modules/html-to-text/node_modules:/home/aaron/sandbox/learnNode/starter-files/node_modules/.pnpm/html-to-text@8.0.0/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../he@1.2.0/node_modules/he/bin/he" "$@"
else
  exec node  "$basedir/../../../../../he@1.2.0/node_modules/he/bin/he" "$@"
fi
