#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/aaron/sandbox/learnNode/starter-files/node_modules/.pnpm/envinfo@7.8.1/node_modules/envinfo/dist/node_modules:/home/aaron/sandbox/learnNode/starter-files/node_modules/.pnpm/envinfo@7.8.1/node_modules/envinfo/node_modules:/home/aaron/sandbox/learnNode/starter-files/node_modules/.pnpm/envinfo@7.8.1/node_modules:/home/aaron/sandbox/learnNode/starter-files/node_modules/.pnpm/node_modules:/home/aaron/sandbox/learnNode/starter-files/node_modules:/home/aaron/sandbox/learnNode/node_modules:/home/aaron/sandbox/node_modules:/home/aaron/node_modules:/home/node_modules:/node_modules:/home/aaron/sandbox/learnNode/starter-files/node_modules/.pnpm/@webpack-cli+info@1.4.1_webpack-cli@4.9.1/node_modules/@webpack-cli/info/node_modules:/home/aaron/sandbox/learnNode/starter-files/node_modules/.pnpm/@webpack-cli+info@1.4.1_webpack-cli@4.9.1/node_modules/@webpack-cli/node_modules:/home/aaron/sandbox/learnNode/starter-files/node_modules/.pnpm/@webpack-cli+info@1.4.1_webpack-cli@4.9.1/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/aaron/sandbox/learnNode/starter-files/node_modules/.pnpm/envinfo@7.8.1/node_modules/envinfo/dist/node_modules:/home/aaron/sandbox/learnNode/starter-files/node_modules/.pnpm/envinfo@7.8.1/node_modules/envinfo/node_modules:/home/aaron/sandbox/learnNode/starter-files/node_modules/.pnpm/envinfo@7.8.1/node_modules:/home/aaron/sandbox/learnNode/starter-files/node_modules/.pnpm/node_modules:/home/aaron/sandbox/learnNode/starter-files/node_modules:/home/aaron/sandbox/learnNode/node_modules:/home/aaron/sandbox/node_modules:/home/aaron/node_modules:/home/node_modules:/node_modules:/home/aaron/sandbox/learnNode/starter-files/node_modules/.pnpm/@webpack-cli+info@1.4.1_webpack-cli@4.9.1/node_modules/@webpack-cli/info/node_modules:/home/aaron/sandbox/learnNode/starter-files/node_modules/.pnpm/@webpack-cli+info@1.4.1_webpack-cli@4.9.1/node_modules/@webpack-cli/node_modules:/home/aaron/sandbox/learnNode/starter-files/node_modules/.pnpm/@webpack-cli+info@1.4.1_webpack-cli@4.9.1/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../envinfo@7.8.1/node_modules/envinfo/dist/cli.js" "$@"
else
  exec node  "$basedir/../../../../../../envinfo@7.8.1/node_modules/envinfo/dist/cli.js" "$@"
fi
