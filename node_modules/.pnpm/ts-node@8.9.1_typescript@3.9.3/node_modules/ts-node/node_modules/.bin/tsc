#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/aaron/sandbox/learnNode/starter-files/node_modules/.pnpm/typescript@3.9.3/node_modules/typescript/bin/node_modules:/home/aaron/sandbox/learnNode/starter-files/node_modules/.pnpm/typescript@3.9.3/node_modules/typescript/node_modules:/home/aaron/sandbox/learnNode/starter-files/node_modules/.pnpm/typescript@3.9.3/node_modules:/home/aaron/sandbox/learnNode/starter-files/node_modules/.pnpm/node_modules:/home/aaron/sandbox/learnNode/starter-files/node_modules:/home/aaron/sandbox/learnNode/node_modules:/home/aaron/sandbox/node_modules:/home/aaron/node_modules:/home/node_modules:/node_modules:/home/aaron/sandbox/learnNode/starter-files/node_modules/.pnpm/ts-node@8.9.1_typescript@3.9.3/node_modules/ts-node/node_modules:/home/aaron/sandbox/learnNode/starter-files/node_modules/.pnpm/ts-node@8.9.1_typescript@3.9.3/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/aaron/sandbox/learnNode/starter-files/node_modules/.pnpm/typescript@3.9.3/node_modules/typescript/bin/node_modules:/home/aaron/sandbox/learnNode/starter-files/node_modules/.pnpm/typescript@3.9.3/node_modules/typescript/node_modules:/home/aaron/sandbox/learnNode/starter-files/node_modules/.pnpm/typescript@3.9.3/node_modules:/home/aaron/sandbox/learnNode/starter-files/node_modules/.pnpm/node_modules:/home/aaron/sandbox/learnNode/starter-files/node_modules:/home/aaron/sandbox/learnNode/node_modules:/home/aaron/sandbox/node_modules:/home/aaron/node_modules:/home/node_modules:/node_modules:/home/aaron/sandbox/learnNode/starter-files/node_modules/.pnpm/ts-node@8.9.1_typescript@3.9.3/node_modules/ts-node/node_modules:/home/aaron/sandbox/learnNode/starter-files/node_modules/.pnpm/ts-node@8.9.1_typescript@3.9.3/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../typescript@3.9.3/node_modules/typescript/bin/tsc" "$@"
else
  exec node  "$basedir/../../../../../typescript@3.9.3/node_modules/typescript/bin/tsc" "$@"
fi
