#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/aaron/sandbox/learnNode/starter-files/node_modules/.pnpm/node-sass@6.0.1/node_modules/node-sass/bin/node_modules:/home/aaron/sandbox/learnNode/starter-files/node_modules/.pnpm/node-sass@6.0.1/node_modules/node-sass/node_modules:/home/aaron/sandbox/learnNode/starter-files/node_modules/.pnpm/node-sass@6.0.1/node_modules:/home/aaron/sandbox/learnNode/starter-files/node_modules/.pnpm/node_modules:/home/aaron/sandbox/learnNode/starter-files/node_modules:/home/aaron/sandbox/learnNode/node_modules:/home/aaron/sandbox/node_modules:/home/aaron/node_modules:/home/node_modules:/node_modules:/home/aaron/sandbox/learnNode/starter-files/node_modules/node-sass/bin/node_modules:/home/aaron/sandbox/learnNode/starter-files/node_modules/node-sass/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/aaron/sandbox/learnNode/starter-files/node_modules/.pnpm/node-sass@6.0.1/node_modules/node-sass/bin/node_modules:/home/aaron/sandbox/learnNode/starter-files/node_modules/.pnpm/node-sass@6.0.1/node_modules/node-sass/node_modules:/home/aaron/sandbox/learnNode/starter-files/node_modules/.pnpm/node-sass@6.0.1/node_modules:/home/aaron/sandbox/learnNode/starter-files/node_modules/.pnpm/node_modules:/home/aaron/sandbox/learnNode/starter-files/node_modules:/home/aaron/sandbox/learnNode/node_modules:/home/aaron/sandbox/node_modules:/home/aaron/node_modules:/home/node_modules:/node_modules:/home/aaron/sandbox/learnNode/starter-files/node_modules/node-sass/bin/node_modules:/home/aaron/sandbox/learnNode/starter-files/node_modules/node-sass/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../node-sass/bin/node-sass" "$@"
else
  exec node  "$basedir/../node-sass/bin/node-sass" "$@"
fi
